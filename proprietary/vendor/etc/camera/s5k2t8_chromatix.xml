      <ISPCommon>s5k2t8_common</ISPCommon>
      <PostProc>s5k2t8_postproc</PostProc>
    </ChromatixName>
    <ChromatixName special_mode_mask = "ZOOM_UPSCALE">
      <CPPSnapshot>s5k2t8_cpp_snapshot_upscale</CPPSnapshot>
    </ChromatixName>
    <ChromatixName special_mode_mask = "ZOOM_DOWNSCALE">
